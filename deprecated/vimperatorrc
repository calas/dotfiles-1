" Search provider
set defsearch=qwant

" Set hintkeys according to my home row in my keyboard layout
set hintchars=asdfhjkl

" Autocomplete using Firefox location bar entries and search engines
set complete=ls

" Select the longest autocomplete match
set wildmode='list:full'

